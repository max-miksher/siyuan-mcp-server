{
  "name": "plugin-sample-vite-vue",
  "version": "0.1.2",
  "type": "module",
  "description": "This is a sample plugin based on vite and vue for Siyuan (https://b3log.org/siyuan)",
  "author": "",
  "license": "MIT",
  "homepage": "",
  "repository": "",
  "scripts": {
    "dev": "vite build --watch",
    "release": "node ./release.js",
    "release:manual": "node ./release.js --mode=manual",
    "release:patch": "node ./release.js --mode=patch",
    "release:minor": "node ./release.js --mode=minor",
    "release:major": "node ./release.js --mode=major",
    "build": "vite build",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.3",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "vue": "^3.5.16",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^24.0.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-perfectionist": "^4.14.0",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "jest": "^30.0.0",
    "jest-junit": "^16.0.0",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.89.2",
    "siyuan": "1.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.0.2",
    "vite-plugin-zip-pack": "^1.2.4"
  }
